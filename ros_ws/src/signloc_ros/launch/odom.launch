<launch>

  <param name="odomTopic" value="/spot/odometry"/>
  <param name="particleTopic" value="/MCLParticles"/>
    <param name="address" value="3 research link, Singapore 119391"/>
  <param name="latlonTopic" value="/gps/fix"/>

  <node pkg="graphloc_ros" type="ODOMLiveTestNode.py" name="ODOMLiveTestNode" output="screen"/>


<node pkg="graphloc_ros" type="NavSatPublisher.py" name="NavSatPublisher" output="screen"/>

<node pkg="graphloc_ros" type="BroadcastNavGraphNode.py" name="BroadcastNavGraphNode" output="screen"/>


    <!-- Start rviz with a pre-configured AerialMap instance. It will use the fake GPS fix from above. -->
   <node pkg="rviz" type="rviz" name="rviz" args="-d $(find graphloc_ros)/rviz/demo.rviz"/> 

    <!-- Static fake TF transform -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="static_tf_fake" args="0 0 0 0 0 0 map base_link" />

</launch>